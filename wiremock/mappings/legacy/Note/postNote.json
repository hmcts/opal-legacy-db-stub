{
  "request": {
    "method": "POST",
    "urlPathPattern": "/opal",
    "queryParameters": {
      "actionType": {
        "equalTo": "LIBRA.add_note"
      }
    }
  },
  "bodyPatterns": [
    {
      "matchesJsonPath": "$[?(@.version != 5)]"
    }
  ],
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/xml"
    },
    "bodyFileName": "legacy/Note/addNote.xml"
  }
}
